{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "MY_KEY": "this is the value",
    "SECRET": "@my_secret_name"
  },
  "scope": "my-team",
  "alias": ["my-domain.com", "my-alias"]
}
